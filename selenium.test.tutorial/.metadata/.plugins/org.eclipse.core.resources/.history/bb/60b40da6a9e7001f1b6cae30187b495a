package de.selenium.projekt1;

public class Auto {

	int kilometerStandAktuell;
	int kilometerStandBevor;
	double liter;
	
	double berechneBenzinVerbrauch() {
		return (kilometerStandAktuell- kilometerStandBevor)/liter;
		
		
	}
